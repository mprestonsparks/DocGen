{
  "name": "docgen",
  "version": "0.1.0",
  "description": "Documentation generator with academic paper implementation support",
  "main": "dist/index.js",
  "bin": {
    "docgen": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist",
    "prepublish": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "documentation",
    "generator",
    "paper",
    "academic",
    "research"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0",
    "commander": "^9.5.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "glob": "^8.1.0",
    "inquirer": "^8.2.6",
    "lodash": "^4.17.21",
    "marked": "^4.3.0",
    "ora": "^5.4.1",
    "pdf-parse": "^1.1.1",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/handlebars": "^4.1.0",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.16",
    "@types/marked": "^4.3.2",
    "@types/node": "^18.19.83",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
